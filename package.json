{
  "name": "@candlefw/css",
  "version": "0.0.8",
  "description": "CSS Parser",
  "main": "source/css_parser.mjs",
  "scripts": {
    "test": "mocha test/test.js -r esm",
    "test.watch": "mocha test/test.js -r esm --watch",
    "build": "rollup --config ./build/config/config.js",
    "build.watch": "rollup --config  ./build/config/config.js --watch",
    "build.ui.watch": "rollup --config  ./build/config/ui.config.js --watch",
    "compile": "hydrocarbon compile -u -o ./source/Parser -t mjs ./source/grammar/css.hcg",
    "compile.mount": "hydrocarbon compile -d -m ./source/grammar/css.hcg",
    "prepare": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candlefw/css.git"
  },
  "keywords": [
    "CSS",
    "Parser"
  ],
  "author": "Anthony Weathersby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/candlefw/css/issues"
  },
  "homepage": "https://github.com/candlefw/css#readme",
  "dependencies": {
    "@candlefw/color": "latest",
    "@candlefw/ll": "latest",
    "@candlefw/math": "latest",
    "@candlefw/url": "latest",
    "@candlefw/whind": "latest"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "esm": "^3.0.84",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-resolve": "0.0.1-predev.1"
  },
  "jshintConfig": {
    "esversion": 6,
    "unused": true
  }
}
